{
  "private": true,
  "name": "@topinerd-design-system/resource-extractor",
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "build": "pnpm build:ui && pnpm build:plugin",
    "build:ui": "vite build -c ./vite.config.ui.js",
    "build:plugin": "vite build -c ./vite.config.plugin.js",
    "dev": "pnpm build:ui --watch && pnpm build:plugin --watch",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "^1.103.0",
    "@topinerd-design-system/eslint-config": "workspace:*",
    "@topinerd-design-system/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/uuid": "^10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-singlefile": "^2.0.3",
    "vite": "catalog:",
    "vitest": "catalog:",
    "typescript": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "postcss": "catalog:",
    "@vitejs/plugin-react": "catalog:"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "lucide-react": "^0.427.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router-dom": "^6.26.0",
    "uuid": "^10.0.0",
    "zustand": "^4.5.4"
  },
  "license": "MIT"
}
